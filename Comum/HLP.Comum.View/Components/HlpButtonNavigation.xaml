<UserControl x:Class="HLP.Comum.View.Components.HlpButtonNavigation"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="50" d:DesignWidth="50">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/HLP.Comum.Resources;component/Styles/Main/MainWindow_Templates.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Button>
        <Button.Content>
            <DockPanel LastChildFill="True">                
                <TextBlock x:Name="txtId" DockPanel.Dock="Bottom"></TextBlock>
                <TextBlock x:Name="txtOpcional"></TextBlock>
            </DockPanel>
        </Button.Content>
    </Button>
</UserControl>
