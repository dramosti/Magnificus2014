<Window x:Class="HLP.Magnificus.View.WPF.WinSobre"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Sobre" Height="300" Width="300" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="15*"></RowDefinition>
            <RowDefinition  Height="85*"></RowDefinition>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions></Grid.ColumnDefinitions>
        <TextBlock Text="MAGNIFICUS" FontSize="25"></TextBlock>
        <WrapPanel Background="{StaticResource StyleMainGridDefault}"
                   Grid.Row="1"
                   Orientation="Vertical">
            <TextBlock Text="Magnificus"></TextBlock>
            <TextBlock x:Name="xVersao" Text=""></TextBlock>
            <TextBlock Text="@HLP Estratégia em Sistema"></TextBlock>
            <TextBlock Text="Todos direitos reservados"></TextBlock>
        </WrapPanel>
        
    </Grid>
</Window>
