<Window x:Class="HLP.Entries.View.WPF.Gerais.WinCidade"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Cidade" Height="300" Width="300">
    <Grid>
        <Label Content="Id" HorizontalAlignment="Left" Margin="45,31,0,0" VerticalAlignment="Top"/>
        <Label Content="Cidade" HorizontalAlignment="Left" Margin="18,79,0,0" VerticalAlignment="Top"/>
        <Label Content="Ibge" HorizontalAlignment="Left" Margin="31,129,0,0" VerticalAlignment="Top"/>
        <Label Content="Uf" HorizontalAlignment="Left" Margin="43,184,0,0" VerticalAlignment="Top" RenderTransformOrigin="-2.394,0.514"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="83,35,0,0" TextWrapping="Wrap" Text="{Binding _currentCidade.idCidade, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}" VerticalAlignment="Top" Width="120"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="83,83,0,0" TextWrapping="Wrap" Text="{Binding _currentCidade.xCidade, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}" VerticalAlignment="Top" Width="120"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="83,132,0,0" TextWrapping="Wrap" Text="{Binding _currentCidade.cIbge, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
