<runtime:WindowsBase 
        xmlns:System="clr-namespace:System;assembly=mscorlib"
        xmlns:runtime="clr-namespace:HLP.Components.View.WPF;assembly=HLP.Components.View.WPF"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:convert="clr-namespace:HLP.Components.View.WPF.Converter;assembly=HLP.Components.View.WPF"
        xmlns:Components="clr-namespace:HLP.Components.View.WPF;assembly=HLP.Components.View.WPF"
        xmlns:ccx="clr-namespace:HLP.Components.ViewModel.ViewModels;assembly=HLP.Components.ViewModel"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="d"
        x:Class="HLP.Entries.View.WPF.Fiscal.WinTipoOperacao" NameView="vwTipoOperacao"
        Title="Tipo de Operação" Height="660" Width="1200">
    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/HLP.Resources.View.WPF;component/Styles/Components/UserControlStyles.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
            <convert:ComboBoxDisplayConverter x:Key="cbxDisplayConv"/>
            <ObjectDataProvider x:Key="odp" ObjectType="{x:Type ccx:FillComboBoxViewModel}"/>
            <ObjectDataProvider x:Key="lResultUf" ObjectInstance="{StaticResource odp}" MethodName="GetAllValuesToComboBox">
                <ObjectDataProvider.MethodParameters>
                    <System:String>vwUFToComboBox</System:String>
                </ObjectDataProvider.MethodParameters>
            </ObjectDataProvider>
            <convert:MaskConverter x:Key="maskConverter"/>
            <convert:PorcentagemConverter x:Key="porcConverter"/>
        </ResourceDictionary>
    </Window.Resources>
    <Grid>
        <TabControl>
            <TabItem Header="Principal">
                <AdornerDecorator>
                    <Grid>
                        <Expander Header="Geral" IsExpanded="True" Style="{StaticResource ExpanderPadrao}">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="10"></ColumnDefinition>
                                    <ColumnDefinition MaxWidth="250" MinWidth="250"></ColumnDefinition>
                                    <ColumnDefinition MaxWidth="90" MinWidth="90"></ColumnDefinition>
                                    <ColumnDefinition MaxWidth="40" MinWidth="40"></ColumnDefinition>
                                    <ColumnDefinition MaxWidth="300" MinWidth="300"></ColumnDefinition>
                                    <ColumnDefinition></ColumnDefinition>
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="10"></RowDefinition>
                                    <RowDefinition Height="Auto" MaxHeight="23"></RowDefinition>
                                    <RowDefinition Height="Auto" MaxHeight="23"></RowDefinition>
                                    <RowDefinition Height="Auto" MaxHeight="23"></RowDefinition>
                                    <RowDefinition Height="Auto" MaxHeight="23"></RowDefinition>
                                    <RowDefinition Height="Auto" MaxHeight="23"></RowDefinition>
                                    <RowDefinition Height="Auto" MaxHeight="23"></RowDefinition>
                                    <RowDefinition Height="Auto" MaxHeight="23"></RowDefinition>
                                    <RowDefinition Height="Auto" MaxHeight="23"></RowDefinition>
                                    <RowDefinition Height="Auto" MaxHeight="23"></RowDefinition>
                                    <RowDefinition Height="Auto" MaxHeight="23"></RowDefinition>
                                    <RowDefinition Height="Auto" MaxHeight="23"></RowDefinition>
                                    <RowDefinition Height="Auto" MaxHeight="23"></RowDefinition>
                                    <RowDefinition Height="Auto" MaxHeight="23"></RowDefinition>
                                    <RowDefinition Height="Auto" MaxHeight="23"></RowDefinition>
                                    <RowDefinition Height="Auto" MaxHeight="23"></RowDefinition>
                                    <RowDefinition Height="Auto" MaxHeight="23"></RowDefinition>
                                    <RowDefinition Height="Auto" MaxHeight="23"></RowDefinition>
                                    <RowDefinition Height="Auto" MaxHeight="23"></RowDefinition>
                                    <RowDefinition Height="Auto" MaxHeight="23"></RowDefinition>
                                    <RowDefinition Height="Auto" MaxHeight="23"></RowDefinition>
                                </Grid.RowDefinitions>

                                <!--Label-->

                                <Components:CustomTextBlock Text="Código" Grid.Column="1" Grid.Row="1"></Components:CustomTextBlock>
                                <Components:CustomTextBlock Text="Nome" Grid.Column="1" Grid.Row="2"></Components:CustomTextBlock>
                                <Components:CustomTextBlock Text="Tipo de Operação" Grid.Column="1" Grid.Row="3"></Components:CustomTextBlock>
                                <Components:CustomTextBlock Text="Representa faturamento?" Grid.Column="1" Grid.Row="4"></Components:CustomTextBlock>
                                <Components:CustomTextBlock Text="Gera compromisso financeiro?" Grid.Column="1" Grid.Row="5"></Components:CustomTextBlock>
                                <Components:CustomTextBlock Text="Gera comissao de representante?" Grid.Column="1" Grid.Row="6"></Components:CustomTextBlock>
                                <Components:CustomTextBlock Text="É serviço?" Grid.Column="1" Grid.Row="7"></Components:CustomTextBlock>
                                <Components:CustomTextBlock Text="Origem da mercadoria" Grid.Column="1" Grid.Row="8"></Components:CustomTextBlock>
                                <Components:CustomTextBlock Text="Movimenta estoque próprio?" Grid.Column="1" Grid.Row="9"></Components:CustomTextBlock>
                                <Components:CustomTextBlock Text="Movimenta estoque de clientes?" Grid.Column="1" Grid.Row="10"></Components:CustomTextBlock>
                                <Components:CustomTextBlock Text="Movimenta estoque de fornecedores?" Grid.Column="1" Grid.Row="11"></Components:CustomTextBlock>
                                <Components:CustomTextBlock Text="É entrega futura?" Grid.Column="1" Grid.Row="12"></Components:CustomTextBlock>
                                <Components:CustomTextBlock Text="Tipo de industrialização" Grid.Column="1" Grid.Row="13"></Components:CustomTextBlock>
                                <Components:CustomTextBlock Text="CFOP dentro do UF" Grid.Column="1" Grid.Row="14"></Components:CustomTextBlock>
                                <Components:CustomTextBlock Text="CFOP fora do UF" Grid.Column="1" Grid.Row="15"></Components:CustomTextBlock>
                                <Components:CustomTextBlock Text="CFOP para nao contribuinte" Grid.Column="1" Grid.Row="16"></Components:CustomTextBlock>
                                <Components:CustomTextBlock Text="Cadastro ativo?" Grid.Column="1" Grid.Row="17"></Components:CustomTextBlock>

                                <!--Components-->

                                <Components:CustomTextBox stCompPosicao="fieldId"
                                    Grid.Column="2" Grid.Row="1"
                                    IsReadOnly="True"
									Text="{Binding currentModel.idTipoOperacao, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}">
                                </Components:CustomTextBox>
                                <Components:CustomTextBox stCompPosicao="first"
                                    Grid.Column="2" Grid.Row="2" Grid.ColumnSpan="3"
                                    Text="{Binding currentModel.xOperacao, Mode=TwoWay, NotifyOnValidationError=True, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"
									IsEnabled="{Binding bIsEnabled}">
                                </Components:CustomTextBox>
                                <Components:CustomComboBox
                                   Grid.Column="2" Grid.Row="3" Grid.ColumnSpan="2"
                                    SelectedIndex="{Binding currentModel.tpOperacao, Mode=TwoWay, NotifyOnValidationError=True, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"
									IsEnabled="{Binding bIsEnabled}">
                                    <Components:CustomComboBox.Items>
                                        <System:String>0-Entrada</System:String>
                                        <System:String>1-Saida</System:String>
                                        <System:String>2-Produção</System:String>
                                        <System:String>3-Conhecimento de Transporte</System:String>
                                        <System:String>4-Locação</System:String>
                                    </Components:CustomComboBox.Items>
                                </Components:CustomComboBox>
                                <Components:CustomCheckBox
                                    Grid.Column="2" Grid.Row="4"
                                    IsChecked="{Binding currentModel.stRepresentaFaturamento, Mode=TwoWay, NotifyOnValidationError=True, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"
									IsEnabled="{Binding bIsEnabled}">
                                </Components:CustomCheckBox>
                                <Components:CustomCheckBox
                                    Grid.Column="2" Grid.Row="5"
                                    IsChecked="{Binding currentModel.stGeraFinanceiro, Mode=TwoWay, NotifyOnValidationError=True, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"
									IsEnabled="{Binding bIsEnabled}">
                                </Components:CustomCheckBox>
                                <Components:CustomCheckBox
                                    Grid.Column="2" Grid.Row="6"
                                    IsChecked="{Binding currentModel.stGeraComissao, Mode=TwoWay, NotifyOnValidationError=True, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"
									IsEnabled="{Binding bIsEnabled}">
                                </Components:CustomCheckBox>
                                <Components:CustomCheckBox
                                    Grid.Column="2" Grid.Row="7"
                                    IsChecked="{Binding currentModel.stServico, Mode=TwoWay, NotifyOnValidationError=True, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"
									IsEnabled="{Binding bIsEnabled}">
                                </Components:CustomCheckBox>
                                <Components:CustomComboBox
                                    Grid.Column="2" Grid.Row="8" Grid.ColumnSpan="2"
                                    SelectedIndex="{Binding currentModel.stOrigemMercadoria, Mode=TwoWay, NotifyOnValidationError=True, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"
									IsEnabled="{Binding bIsEnabled}">
                                    <Components:CustomComboBox.Items>
                                        <System:String>0-Nacional</System:String>
                                        <System:String>1-Estrangeiro - Importação Direta</System:String>
                                        <System:String>2-Estrangeiro - Adquirida no Mercado Interno</System:String>
                                    </Components:CustomComboBox.Items>
                                </Components:CustomComboBox>
                                <Components:CustomComboBox
                                    Grid.Column="2" Grid.Row="9" Grid.ColumnSpan="2"
                                    SelectedIndex="{Binding currentModel.stEstoque, Mode=TwoWay, NotifyOnValidationError=True, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"
									IsEnabled="{Binding bIsEnabled}">
                                    <Components:CustomComboBox.Items>
                                        <System:String>0-Saida</System:String>
                                        <System:String>1-Entrada</System:String>
                                        <System:String>2-Não Movimenta</System:String>
                                    </Components:CustomComboBox.Items>
                                </Components:CustomComboBox>
                                <Components:CustomComboBox
                                    Grid.Column="2" Grid.Row="10" Grid.ColumnSpan="2"
                                    SelectedIndex="{Binding currentModel.stEstoqueClientes, Mode=TwoWay, NotifyOnValidationError=True, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"
									IsEnabled="{Binding bIsEnabled}">
                                    <Components:CustomComboBox.Items>
                                        <System:String>0-Saida</System:String>
                                        <System:String>1-Entrada</System:String>
                                        <System:String>2-Não Movimenta</System:String>
                                    </Components:CustomComboBox.Items>
                                </Components:CustomComboBox>
                                <Components:CustomComboBox
                                    Grid.Column="2" Grid.Row="11" Grid.ColumnSpan="2"
                                    SelectedIndex="{Binding currentModel.stEstoqueFornecedores, Mode=TwoWay, NotifyOnValidationError=True, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"
									IsEnabled="{Binding bIsEnabled}">
                                    <Components:CustomComboBox.Items>
                                        <System:String>0-Saida</System:String>
                                        <System:String>1-Entrada</System:String>
                                        <System:String>2-Não Movimenta</System:String>
                                    </Components:CustomComboBox.Items>
                                </Components:CustomComboBox>
                                <Components:CustomCheckBox
                                    Grid.Column="2" Grid.Row="12"
                                    IsChecked="{Binding currentModel.stEntregaFutura, Mode=TwoWay, NotifyOnValidationError=True, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"
									IsEnabled="{Binding bIsEnabled}">
                                </Components:CustomCheckBox>
                                <Components:CustomComboBox
                                    Grid.Column="2" Grid.Row="13" Grid.ColumnSpan="2"
                                    SelectedIndex="{Binding currentModel.stTipoIndustrializacao, Mode=TwoWay, NotifyOnValidationError=True, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"
									IsEnabled="{Binding bIsEnabled}">
                                    <Components:CustomComboBox.Items>
                                        <System:String>0-Nenhum</System:String>
                                        <System:String>1-Insumos</System:String>
                                    </Components:CustomComboBox.Items>
                                </Components:CustomComboBox>
                                <Components:HlpPesquisa 
									Grid.Column="2" Grid.Row="14" Height="20" Margin="0,0,0,3" Grid.ColumnSpan="3"
									Text="{Binding currentModel.cCfopNaUf, Mode=TwoWay, NotifyOnValidationError=True, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"
									IsEnabled="{Binding bIsEnabled}"
									WidthLabel="0" FieldPesquisa="ID" TableView="vwCfop" nameWindow="WinCfop">
                                    <Components:HlpPesquisa.Items>
                                        <System:String>cCfop</System:String>
                                        <System:String>xCfop</System:String>
                                    </Components:HlpPesquisa.Items>
                                </Components:HlpPesquisa>
                                <Components:HlpPesquisa 
									Grid.Column="2" Grid.Row="15" Height="20" Margin="0,0,0,3" Grid.ColumnSpan="3"
									Text="{Binding currentModel.cCfopOutraUf, Mode=TwoWay, NotifyOnValidationError=True, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"
									IsEnabled="{Binding bIsEnabled}"
									WidthLabel="0" FieldPesquisa="ID" TableView="vwCfop" nameWindow="WinCfop">
                                    <Components:HlpPesquisa.Items>
                                        <System:String>cCfop</System:String>
                                        <System:String>xCfop</System:String>
                                    </Components:HlpPesquisa.Items>
                                </Components:HlpPesquisa>
                                <Components:HlpPesquisa 
									Grid.Column="2" Grid.Row="16" Height="20" Margin="0,0,0,3" Grid.ColumnSpan="3"
									Text="{Binding currentModel.cCfopNaoContribuinte, Mode=TwoWay, NotifyOnValidationError=True, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"
									IsEnabled="{Binding bIsEnabled}"
									WidthLabel="0" FieldPesquisa="ID" TableView="vwCfop" nameWindow="WinCfop">
                                    <Components:HlpPesquisa.Items>
                                        <System:String>cCfop</System:String>
                                        <System:String>xCfop</System:String>
                                    </Components:HlpPesquisa.Items>
                                </Components:HlpPesquisa>
                                <Components:CustomCheckBox
                                    Grid.Column="2" Grid.Row="17"
                                    IsChecked="{Binding currentModel.Ativo, Mode=TwoWay, NotifyOnValidationError=True, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"
									IsEnabled="{Binding bIsEnabled}">
                                </Components:CustomCheckBox>
                            </Grid>
                        </Expander>
                    </Grid>
                </AdornerDecorator>
            </TabItem>
            <TabItem Header="Entradas">
                <AdornerDecorator>
                    <Grid>
                        <Expander Header="Parâmetros de Entrada" IsExpanded="True" Style="{StaticResource ExpanderPadrao}">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="10"></ColumnDefinition>
                                    <ColumnDefinition MaxWidth="500" MinWidth="500"></ColumnDefinition>
                                    <ColumnDefinition MaxWidth="140" MinWidth="140"></ColumnDefinition>
                                    <ColumnDefinition></ColumnDefinition>
                                    <ColumnDefinition></ColumnDefinition>
                                    <ColumnDefinition></ColumnDefinition>
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="10"></RowDefinition>
                                    <RowDefinition Height="Auto" MaxHeight="23"></RowDefinition>
                                    <RowDefinition Height="Auto" MaxHeight="23"></RowDefinition>
                                    <RowDefinition Height="Auto" MaxHeight="23"></RowDefinition>
                                    <RowDefinition Height="Auto" MaxHeight="23"></RowDefinition>
                                    <RowDefinition Height="Auto" MaxHeight="23"></RowDefinition>
                                    <RowDefinition Height="Auto" MaxHeight="23"></RowDefinition>
                                    <RowDefinition Height="Auto" MaxHeight="23"></RowDefinition>
                                    <RowDefinition Height="Auto" MaxHeight="23"></RowDefinition>
                                    <RowDefinition Height="Auto" MaxHeight="23"></RowDefinition>
                                </Grid.RowDefinitions>

                                <!--Label-->

                                <Components:CustomTextBlock Text="Relação do recebimento com inspeção de qualidade" Grid.Column="1" Grid.Row="1"></Components:CustomTextBlock>
                                <Components:CustomTextBlock Text="Calcula o custo final no recebimento?" Grid.Column="1" Grid.Row="2"></Components:CustomTextBlock>
                                <Components:CustomTextBlock Text="Desconta o crédito de icms na formação do custo final?" Grid.Column="1" Grid.Row="3"></Components:CustomTextBlock>
                                <Components:CustomTextBlock Text="Desconta o crédito de subst. tributária de icms na formação de custo final?" Grid.Column="1" Grid.Row="4"></Components:CustomTextBlock>
                                <Components:CustomTextBlock Text="Desconta o crédito de ipi na formalção de custo final?" Grid.Column="1" Grid.Row="5"></Components:CustomTextBlock>
                                <Components:CustomTextBlock Text="Desconta o crédito de pis/cofins na formação de custo final?" Grid.Column="1" Grid.Row="6"></Components:CustomTextBlock>
                                <Components:CustomTextBlock Text="Desconta o custo da diferença de icms na formação de custo final?" Grid.Column="1" Grid.Row="7"></Components:CustomTextBlock>
                                <Components:CustomTextBlock Text="Obriga informa ordem de produção no precesso de compra?" Grid.Column="1" Grid.Row="8"></Components:CustomTextBlock>

                                <!--Components-->

                                <Components:CustomComboBox
                                    Grid.Column="2" Grid.Row="1"                                  
									SelectedIndex="{Binding currentModel.stRelacaoRecebimentoInspecao, Mode=TwoWay, NotifyOnValidationError=True, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"
									IsEnabled="{Binding bIsEnabled}">
                                    <Components:CustomComboBox.Items>
                                        <System:String>0-Nenhum</System:String>
                                        <System:String>1-Gera Inspeção</System:String>
                                    </Components:CustomComboBox.Items>
                                </Components:CustomComboBox>
                                <Components:CustomCheckBox
                                    Grid.Column="2" Grid.Row="2"
                                    IsChecked="{Binding currentModel.stCalculaCustoEntrada, Mode=TwoWay, NotifyOnValidationError=True, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"
									IsEnabled="{Binding bIsEnabled}">
                                </Components:CustomCheckBox>
                                <Components:CustomCheckBox
                                    Grid.Column="2" Grid.Row="3"
                                    IsChecked="{Binding currentModel.stDescontaCreditoIcmsCusto, Mode=TwoWay, NotifyOnValidationError=True, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"
									IsEnabled="{Binding bIsEnabled}">
                                </Components:CustomCheckBox>
                                <Components:CustomCheckBox
                                    Grid.Column="2" Grid.Row="4"
                                    IsChecked="{Binding currentModel.stDescontaCreditoSubstituicaoTributariaCusto, Mode=TwoWay, NotifyOnValidationError=True, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"
									IsEnabled="{Binding bIsEnabled}">
                                </Components:CustomCheckBox>
                                <Components:CustomCheckBox
                                    Grid.Column="2" Grid.Row="5"
                                    IsChecked="{Binding currentModel.stDescontaCreditoIpiCusto, Mode=TwoWay, NotifyOnValidationError=True, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"
									IsEnabled="{Binding bIsEnabled}">
                                </Components:CustomCheckBox>
                                <Components:CustomCheckBox
                                    Grid.Column="2" Grid.Row="6"
                                    IsChecked="{Binding currentModel.stDescontaCreditoPiscofinsCusto, Mode=TwoWay, NotifyOnValidationError=True, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"
									IsEnabled="{Binding bIsEnabled}">
                                </Components:CustomCheckBox>
                                <Components:CustomCheckBox
                                    Grid.Column="2" Grid.Row="7"
                                    IsChecked="{Binding currentModel.stDescontaDiferencialIcmsCusto, Mode=TwoWay, NotifyOnValidationError=True, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"
									IsEnabled="{Binding bIsEnabled}">
                                </Components:CustomCheckBox>
                                <Components:CustomCheckBox
                                    Grid.Column="2" Grid.Row="8"
                                    IsChecked="{Binding currentModel.stObrigaOpNaCompra, Mode=TwoWay, NotifyOnValidationError=True, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"
									IsEnabled="{Binding bIsEnabled}">
                                </Components:CustomCheckBox>

                            </Grid>
                        </Expander>
                    </Grid>
                </AdornerDecorator>
            </TabItem>
            <TabItem Header="Saídas">
                <AdornerDecorator>
                    <Grid>
                        <Expander Header="Parâmetros de Saída" IsExpanded="True" Style="{StaticResource ExpanderPadrao}">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="10"></ColumnDefinition>
                                    <ColumnDefinition MaxWidth="420" MinWidth="420"></ColumnDefinition>
                                    <ColumnDefinition MaxWidth="90" MinWidth="90"></ColumnDefinition>
                                    <ColumnDefinition></ColumnDefinition>
                                    <ColumnDefinition></ColumnDefinition>
                                    <ColumnDefinition></ColumnDefinition>
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="10"></RowDefinition>
                                    <RowDefinition Height="Auto" MaxHeight="23"></RowDefinition>
                                    <RowDefinition Height="Auto" MaxHeight="23"></RowDefinition>
                                    <RowDefinition Height="Auto" MaxHeight="23"></RowDefinition>
                                    <RowDefinition Height="Auto" MaxHeight="23"></RowDefinition>
                                    <RowDefinition Height="Auto" MaxHeight="23"></RowDefinition>
                                </Grid.RowDefinitions>

                                <!--Label-->

                                <Components:CustomTextBlock Text="Porcentagem de insumo no itme de industrialização" Grid.Column="1" Grid.Row="1"></Components:CustomTextBlock>
                                <Components:CustomTextBlock Text="Porcentagem de mão de obra no itme de industrialização" Grid.Column="1" Grid.Row="2"></Components:CustomTextBlock>
                                <Components:CustomTextBlock Text="Calculo de Importação?" Grid.Column="1" Grid.Row="3"></Components:CustomTextBlock>
                                <Components:CustomTextBlock Text="Calcula crédito de icms no super simples?" Grid.Column="1" Grid.Row="4"></Components:CustomTextBlock>
                                <Components:CustomTextBlock Text="Calcula crédito de icms no super simples e com subst.tributária?" Grid.Column="1" Grid.Row="5"></Components:CustomTextBlock>

                                <!--Components-->

                                <Components:CustomTextBox
                                    Grid.Column="2" Grid.Row="1"
                                    Text="{Binding currentModel.pInsumoIndustrializacao, Converter={StaticResource porcConverter},
                                    Mode=TwoWay, NotifyOnValidationError=True, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"
									IsEnabled="{Binding bIsEnabled}">
                                </Components:CustomTextBox>
                                <Components:CustomTextBox
                                    Grid.Column="2" Grid.Row="2"
                                    Text="{Binding currentModel.pMaoObraIndustrializacao, Mode=TwoWay, Converter={StaticResource porcConverter},
                                    NotifyOnValidationError=True, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"
									IsEnabled="{Binding bIsEnabled}">
                                </Components:CustomTextBox>
                                <Components:CustomCheckBox
                                    Grid.Column="2" Grid.Row="3"
                                    IsChecked="{Binding currentModel.stCalculaImportacao, Mode=TwoWay, NotifyOnValidationError=True, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"
									IsEnabled="{Binding bIsEnabled}">
                                </Components:CustomCheckBox>
                                <Components:CustomCheckBox
                                    Grid.Column="2" Grid.Row="4"
                                    IsChecked="{Binding currentModel.stCalculaCreditoIcmsSuperSimples, Mode=TwoWay, NotifyOnValidationError=True, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"
									IsEnabled="{Binding bIsEnabled}">
                                </Components:CustomCheckBox>
                                <Components:CustomCheckBox
                                    Grid.Column="2" Grid.Row="5"
                                    IsChecked="{Binding currentModel.stCalculaCreditoIcmsSubstituicaoTributariaSuperSimples, Mode=TwoWay, NotifyOnValidationError=True, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"
									IsEnabled="{Binding bIsEnabled}">
                                </Components:CustomCheckBox>

                            </Grid>
                        </Expander>
                    </Grid>
                </AdornerDecorator>
            </TabItem>
            <TabItem Header="Fiscal Geral">
                <AdornerDecorator>
                    <TabControl>
                        <TabItem Header="Fiscal/Serviço">
                            <AdornerDecorator>
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto"></RowDefinition>
                                        <RowDefinition Height="Auto"></RowDefinition>
                                    </Grid.RowDefinitions>
                                    <Expander Header="Fiscal" Style="{StaticResource ExpanderPadrao}" IsExpanded="True">
                                        <Grid>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="10"></ColumnDefinition>
                                                <ColumnDefinition MaxWidth="250" MinWidth="250"></ColumnDefinition>
                                                <ColumnDefinition MaxWidth="130" MinWidth="130"></ColumnDefinition>
                                                <ColumnDefinition MaxWidth="200" MinWidth="200"></ColumnDefinition>
                                                <ColumnDefinition></ColumnDefinition>
                                                <ColumnDefinition></ColumnDefinition>
                                            </Grid.ColumnDefinitions>
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="10"></RowDefinition>
                                                <RowDefinition Height="Auto" MaxHeight="23"></RowDefinition>
                                                <RowDefinition Height="Auto" MaxHeight="23"></RowDefinition>
                                                <RowDefinition Height="Auto" MaxHeight="23"></RowDefinition>
                                                <RowDefinition Height="Auto" MaxHeight="23"></RowDefinition>
                                                <RowDefinition Height="Auto" MaxHeight="23"></RowDefinition>
                                                <RowDefinition Height="Auto" MaxHeight="23"></RowDefinition>
                                                <RowDefinition Height="Auto" MaxHeight="23"></RowDefinition>
                                                <RowDefinition Height="Auto" MaxHeight="23"></RowDefinition>
                                            </Grid.RowDefinitions>
                                            
                                            <!--Label-->

                                            <Components:CustomTextBlock Text="Reduz base de cálculo?" Grid.Column="1" Grid.Row="1"></Components:CustomTextBlock>
                                            <Components:CustomTextBlock Text="Quando não reduz base de cálculo?" Grid.Column="1" Grid.Row="2"></Components:CustomTextBlock>
                                            <Components:CustomTextBlock Text="Calcula ICMS?" Grid.Column="1" Grid.Row="3"></Components:CustomTextBlock>
                                            <Components:CustomTextBlock Text="Código de ICMS" Grid.Column="1" Grid.Row="4"></Components:CustomTextBlock>
                                            <Components:CustomTextBlock Text="Situação tributária de ICMS" Grid.Column="1" Grid.Row="5"></Components:CustomTextBlock>
                                            <Components:CustomTextBlock Text="Compõe a base de cálculo de ICMS?" Grid.Column="1" Grid.Row="6"></Components:CustomTextBlock>
                                            <Components:CustomTextBlock Text="Calcula IPI" Grid.Column="1" Grid.Row="7"></Components:CustomTextBlock>
                                            <Components:CustomTextBlock Text="Classificação fiscal" Grid.Column="1" Grid.Row="8"></Components:CustomTextBlock>
                                            
                                            <!--Components-->
                                            
                                            <Components:CustomComboBox
                                                Grid.Column="2" Grid.Row="1"
                                                SelectedIndex="{Binding currentModel.stReduzBase, Mode=TwoWay, NotifyOnValidationError=True, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"
