<runtime:WindowsBase x:Class="HLP.Sales.View.WPF.GestãoAdministrativa.Comercial.WinOrcamento"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:runtime="clr-namespace:HLP.Components.View.WPF;assembly=HLP.Components.View.WPF"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:System="clr-namespace:System;assembly=mscorlib"
        xmlns:convert="clr-namespace:HLP.Components.View.WPF.Converter;assembly=HLP.Components.View.WPF"                     
        xmlns:convertLocal="clr-namespace:HLP.Sales.View.WPF.Converters"
        xmlns:Components="clr-namespace:HLP.Components.View.WPF;assembly=HLP.Components.View.WPF"
        xmlns:dat="clr-namespace:System.Windows.Data;assembly=PresentationFramework"
        xmlns:ccx="clr-namespace:HLP.Components.ViewModel.ViewModels;assembly=HLP.Components.ViewModel"
        xmlns:util="clr-namespace:HLP.Base.ClassesBases;assembly=HLP.Base"
        Title="Orçamento de Vendas" Height="660" Width="1200" NameView="vwOrcamento">
    <runtime:WindowsBase.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/HLP.Resources.View.WPF;component/Styles/Components/UserControlStyles.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
            <convert:SelectedItemToTextConverter x:Key="selectedItemToTextConverter"/>
            <CompositeCollection x:Key="origemMercadoria">
                <System:String>0-Nacional</System:String>
                <System:String>1-Estrangeira – Importação Direta</System:String>
                <System:String>2-Estrangeira – Adquirida no mercado Interno</System:String>
                <System:String>3-Nacional, mercadoria ou bem com Conteúdo de Importação superior a 40% (quarenta por cento)</System:String>
                <System:String>4-Nacional, cuja produção tenha sido feita em conformidade com os processos produtivos básicos de que tratam o Decreto-Lei nº 288/67, e as Leis nºs 8.248/91, 8.387/91, 10.176/01 e 11 . 4 8 4 / 0 7</System:String>
                <System:String>5-Nacional, mercadoria ou bem com Conteúdo de Importação inferior ou igual a 40% (quarenta por cento)</System:String>
                <System:String>6-Estrangeira - Importação direta, sem similar nacional, constante em lista de Resolução CAMEX</System:String>
                <System:String>7-Estrangeira - Adquirida no mercado interno, sem similar nacional, constante em lista de Resolução CAMEX</System:String>
            </CompositeCollection>
            <convert:NotConverter x:Key="notConverter"></convert:NotConverter>
            <convertLocal:StatusOrcamentoConverter x:Key="statusOrcamentoConv"></convertLocal:StatusOrcamentoConverter>
            <convertLocal:CanUserAddOrcamentoItemConverter x:Key="canUserAddConv"></convertLocal:CanUserAddOrcamentoItemConverter>
            <CollectionViewSource 
    Source="{Binding currentModel.lOrcamento_Itens, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" 
    x:Key="cvsItens"
    x:Name="cvsItens"
    CollectionViewType="{x:Type dat:ListCollectionView}" />
            <CollectionViewSource 
    Source="{Binding currentModel.lOrcamento_Item_Impostos, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" 
    x:Key="cvsImpostos"
    x:Name="cvsImpostos"
    CollectionViewType="{x:Type dat:ListCollectionView}" />
            <CollectionViewSource Source="{Binding itensComissoes}" x:Key="cvsComissoes">
                <CollectionViewSource.GroupDescriptions>
                    <PropertyGroupDescription PropertyName="xRepresentante"></PropertyGroupDescription>
                </CollectionViewSource.GroupDescriptions>
            </CollectionViewSource>
            <convert:ComboBoxDisplayConverter x:Key="cbxDisplayConv"/>
            <convertLocal:IdMotivoVisibilityConverter x:Key="idMotivoVisibilityConv"></convertLocal:IdMotivoVisibilityConverter>
            <convert:ByteToBoolConverter x:Key="ByteToBoolConverter"/>
            <convert:PorcentagemConverter x:Key="porcentagemConverter"/>
            <convert:MoedaConverter x:Key="moedaConverter"/>
            <util:BindingProxy x:Key="proxy" Data="{Binding}" />
            <ObjectDataProvider x:Key="odp" ObjectType="{x:Type ccx:FillComboBoxViewModel}"/>
            <ObjectDataProvider x:Key="lresultPerdaCanc" ObjectInstance="{StaticResource odp}" MethodName="GetAllValuesToComboBox">
                <ObjectDataProvider.MethodParameters>
                    <System:String>vwMotivo_PerdaCanc_OrcamentoToComboBox</System:String>
                </ObjectDataProvider.MethodParameters>
            </ObjectDataProvider>
            <ObjectDataProvider x:Key="lResultDeposito" ObjectInstance="{StaticResource odp}" MethodName="GetAllValuesToComboBox">
                <ObjectDataProvider.MethodParameters>
                    <System:String>vwDepositoToComboBox</System:String>
                </ObjectDataProvider.MethodParameters>
            </ObjectDataProvider>
            <ObjectDataProvider x:Key="lResultSite" ObjectInstance="{StaticResource odp}" MethodName="GetAllValuesToComboBox">
                <ObjectDataProvider.MethodParameters>
                    <System:String>vwSiteToComboBox</System:String>
                </ObjectDataProvider.MethodParameters>
            </ObjectDataProvider>
            <ObjectDataProvider x:Key="lResultUnMedida" ObjectInstance="{StaticResource odp}" MethodName="GetAllValuesToComboBox">
                <ObjectDataProvider.MethodParameters>
                    <System:String>vwUnidadeMedidaToComboBox</System:String>
                </ObjectDataProvider.MethodParameters>
            </ObjectDataProvider>
            <ObjectDataProvider x:Key="lResultCstIcms" ObjectInstance="{StaticResource odp}" MethodName="GetAllValuesToComboBox">
                <ObjectDataProvider.MethodParameters>
                    <System:String>vwSituacao_tributaria_IcmsToComboBox</System:String>
                </ObjectDataProvider.MethodParameters>
            </ObjectDataProvider>
            <ObjectDataProvider x:Key="lResultStPis" ObjectInstance="{StaticResource odp}" MethodName="GetAllValuesToComboBox">
                <ObjectDataProvider.MethodParameters>
                    <System:String>vwStPisToComboBox</System:String>
                </ObjectDataProvider.MethodParameters>
            </ObjectDataProvider>
            <ObjectDataProvider x:Key="lResultiCofins" ObjectInstance="{StaticResource odp}" MethodName="GetAllValuesToComboBox">
                <ObjectDataProvider.MethodParameters>
                    <System:String>vwStCofinsToComboBox</System:String>
                </ObjectDataProvider.MethodParameters>
            </ObjectDataProvider>
            <ObjectDataProvider x:Key="lResultIcmsPai" ObjectInstance="{StaticResource odp}" MethodName="GetAllValuesToComboBox">
                <ObjectDataProvider.MethodParameters>
                    <System:String>vwCodicoIcmsPaiToComboBox</System:String>
                </ObjectDataProvider.MethodParameters>
            </ObjectDataProvider>
            <ObjectDataProvider x:Key="lResultListaPreco" ObjectInstance="{StaticResource odp}" MethodName="GetAllValuesToComboBox">
                <ObjectDataProvider.MethodParameters>
                    <System:String>vwListaPrecoToComboBox</System:String>
                </ObjectDataProvider.MethodParameters>
            </ObjectDataProvider>
            <ObjectDataProvider x:Key="lResultUnidadeMedida" ObjectInstance="{StaticResource odp}" MethodName="GetAllValuesToComboBox">
                <ObjectDataProvider.MethodParameters>
                    <System:String>getUnidadeMedidaToComboBox</System:String>
                    <System:String>0</System:String>
                </ObjectDataProvider.MethodParameters>
            </ObjectDataProvider>
            <ObjectDataProvider x:Key="lResultContatos" ObjectInstance="{StaticResource odp}" MethodName="GetAllValuesToComboBox">
                <ObjectDataProvider.MethodParameters>
                    <System:String>getAuthorsToComboBox</System:String>
                    <System:String>0</System:String>
                </ObjectDataProvider.MethodParameters>
            </ObjectDataProvider>
            <ObjectDataProvider x:Key="lResultTipoDocumento" ObjectInstance="{StaticResource odp}" MethodName="GetAllValuesToComboBox">
                <ObjectDataProvider.MethodParameters>
                    <System:String>vwTipoDocumentoToComboBoxOrcamento</System:String>
                </ObjectDataProvider.MethodParameters>
            </ObjectDataProvider>
            <ObjectDataProvider x:Key="lResultTipoOperacao" ObjectInstance="{StaticResource odp}" MethodName="GetAllValuesToComboBox">
                <ObjectDataProvider.MethodParameters>
                    <System:String>vwTipoOperacaoToComboBox</System:String>
                </ObjectDataProvider.MethodParameters>
            </ObjectDataProvider>
            <ObjectDataProvider x:Key="lresultProdutos" ObjectInstance="{StaticResource odp}" MethodName="GetAllValuesToComboBox">
                <ObjectDataProvider.MethodParameters>
                    <System:String>vwProdutoToComboBox</System:String>
                </ObjectDataProvider.MethodParameters>
            </ObjectDataProvider>
            <ObjectDataProvider x:Key="lResultRepresentantes" ObjectInstance="{StaticResource odp}" MethodName="GetAllValuesToComboBox">
                <ObjectDataProvider.MethodParameters>
                    <System:String>vwFuncionarioRepresentanteToComboBox</System:String>
                </ObjectDataProvider.MethodParameters>
            </ObjectDataProvider>
            <ObjectDataProvider x:Key="lResultCfop" ObjectInstance="{StaticResource odp}" MethodName="GetAllValuesToComboBox">
                <ObjectDataProvider.MethodParameters>
                    <System:String>vwCfopToComboBox</System:String>
                </ObjectDataProvider.MethodParameters>
            </ObjectDataProvider>
            <ObjectDataProvider x:Key="lResultClassifFiscal" ObjectInstance="{StaticResource odp}" MethodName="GetAllValuesToComboBox">
                <ObjectDataProvider.MethodParameters>
                    <System:String>vwClassificacaoFiscalToComboBox</System:String>
                </ObjectDataProvider.MethodParameters>
            </ObjectDataProvider>
            <ObjectDataProvider x:Key="lResultCstIpi" ObjectInstance="{StaticResource odp}" MethodName="GetAllValuesToComboBox">
                <ObjectDataProvider.MethodParameters>
                    <System:String>vwSituacaoTributaria_IpiToComboBox</System:String>
                </ObjectDataProvider.MethodParameters>
            </ObjectDataProvider>
            <convert:MaskConverter x:Key="maskConverter"/>
        </ResourceDictionary>
    </runtime:WindowsBase.Resources>
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Expander Header="Cabeçalho do Orçamento" IsExpanded="True" Style="{StaticResource ExpanderPadrao}">
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="10"></ColumnDefinition>
                    <ColumnDefinition MaxWidth="90" MinWidth="90"></ColumnDefinition>
                    <ColumnDefinition Width="5"></ColumnDefinition>
                    <ColumnDefinition MaxWidth="130"></ColumnDefinition>
                    <ColumnDefinition Width="5"></ColumnDefinition>
                    <ColumnDefinition MaxWidth="250" MinWidth="250"></ColumnDefinition>
                    <ColumnDefinition Width="5"></ColumnDefinition>
                    <ColumnDefinition MaxWidth="250" MinWidth="250"></ColumnDefinition>
                    <ColumnDefinition Width="5"></ColumnDefinition>
                    <ColumnDefinition></ColumnDefinition>
                    <ColumnDefinition Width="5"></ColumnDefinition>
                    <ColumnDefinition></ColumnDefinition>
                    <ColumnDefinition Width="10"></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="10"></RowDefinition>
                    <RowDefinition Height="Auto" MaxHeight="23"></RowDefinition>
                    <RowDefinition Height="Auto" MaxHeight="23"></RowDefinition>
                    <RowDefinition Height="Auto" MaxHeight="23"></RowDefinition>
                    <RowDefinition Height="Auto" MaxHeight="23"></RowDefinition>
                </Grid.RowDefinitions>

                <!--Label-->
                <Components:CustomHlpStatus Grid.Column="9" Grid.ColumnSpan="3" Grid.RowSpan="3" HorizontalAlignment="Right"
                                            Height="50" VerticalAlignment="Center" selectedStatus="{Binding currentModel.stOrcamento}"
                                            >
                    <Components:CustomHlpStatus.lStatusInterrompidos>
                        <System:Int32>3</System:Int32>
                        <System:Int32>4</System:Int32>
                    </Components:CustomHlpStatus.lStatusInterrompidos>
                    <Components:CustomHlpStatus.lStatus>
                        <System:String>Criado</System:String>
                        <System:String>Enviado</System:String>
                        <System:String>Confirmado</System:String>
                    </Components:CustomHlpStatus.lStatus>
                </Components:CustomHlpStatus>
                <Components:CustomHlpVersoes Grid.Column="9" Grid.ColumnSpan="3" Grid.Row="3" Height="50" Grid.RowSpan="2"
                                             ItemsSourceVersao="{Binding lItensHierarquia, Mode=TwoWay}"></Components:CustomHlpVersoes>
                <Components:CustomTextBlock Text="Código" Grid.Column="1" Grid.Row="1"></Components:CustomTextBlock>
                <Components:CustomTextBlock Text="Data e Hora Criada" Grid.Column="3" Grid.Row="1"></Components:CustomTextBlock>
                <Components:CustomTextBlock Text="Cliente" Grid.Column="5" Grid.Row="1"></Components:CustomTextBlock>
                <Components:CustomTextBlock Text="Tipo de Documento" Grid.Column="7" Grid.Row="1"></Components:CustomTextBlock>
                <Components:CustomTextBlock Text="Empresa" Grid.Column="1" Grid.Row="3"></Components:CustomTextBlock>
                <Components:CustomTextBlock Text="Representante" Grid.Column="5" Grid.Row="3"></Components:CustomTextBlock>
                <Components:CustomTextBlock Text="Condição de Pagamento" Grid.Column="7" Grid.Row="3"></Components:CustomTextBlock>

                <!--Components-->
                <Components:CustomTextBox
                    Grid.Column="1" Grid.Row="2"
                    IsReadOnly="True"
					Text="{Binding currentModel.idOrcamento, Mode=TwoWay,  UpdateSourceTrigger=PropertyChanged}"
					ToolTip="O número de identificação automático e exclusivo do orçamento de venda.">
                </Components:CustomTextBox>
                <Components:HlpDatePicker
                    Grid.Column="3" Grid.Row="2" Height="20" Margin="0,0,0,3"
                    xTextDate="{Binding currentModel.dDataHora, Mode=TwoWay, NotifyOnValidationError=True, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"
					IsEnabled="False"
					ToolTip="O Sistema preenche automaticamente com a data e hora (dd/mm/aaaa - hh:mm) em que foi criado o orçamento.">
                </Components:HlpDatePicker>
                <Components:ucTextBoxIntellisense
                    x:Name="pesquisaCliente" Height="20" Margin="0,0,0,3" Grid.Column="5" Grid.Row="2"                   
					selectedId="{Binding currentModel.idClienteFornecedor, Mode=TwoWay, NotifyOnValidationError=True, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"
					IsEnabled="{Binding bIsEnabled}" refMethod="{Binding getCliente}"
					ToolTip="Informar ao orçamento de vendas o cliente." xNameView="vwClienteqs"
                    model="{Binding currentModel.objCliente}" Loaded="pesquisaCliente_Loaded"
                    ></Components:ucTextBoxIntellisense>
                <Components:CustomComboBox Grid.Column="7" Grid.Row="2" IsEnabled="{Binding bIsEnabled}"
                                           ItemsSource="{Binding Source={StaticResource lResultTipoDocumento}}"
                                           SelectedValue="{Binding currentModel.idTipoDocumento, Mode=TwoWay, NotifyOnValidationError=True, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"
                                           SelectedValuePath="id"
                                           DisplayMemberPath="display">

                </Components:CustomComboBox>
                <Components:CustomTextBox
                    Grid.Column="1" Grid.Row="4" Grid.ColumnSpan="3"
                    IsEnabled="False"
					Text="{Binding currentModel.xEmpresa, Mode=TwoWay,  UpdateSourceTrigger=PropertyChanged}"
					ToolTip="Código da Empresa em que o sistema esta lançando o orçamento">
                </Components:CustomTextBox>
                <Components:CustomPesquisa
                    Grid.Column="5" Grid.Row="4" Height="20" Margin="0,0,0,3"
                    xIdPesquisa="{Binding currentModel.idFuncionarioRepresentante, Mode=TwoWay, NotifyOnValidationError=True, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"
					IsEnabled="{Binding bIsEnabled}" NameWindowCadastro="WinFuncionario"
					ToolTip="Informar o representante do orçamento de vendas" TableView="vwFuncionarioRepresentante" FieldPesquisa="ID">
                    <Components:CustomPesquisa.Items>
                        <System:String>xCodigoAlternativo</System:String>
                        <System:String>xNome</System:String>
                    </Components:CustomPesquisa.Items>
                </Components:CustomPesquisa>
                <Components:CustomPesquisa
                    Grid.Column="7" Grid.Row="4" Height="20" Margin="0,0,0,3"
                    xIdPesquisa="{Binding currentModel.idCondicaoPagamento, Mode=TwoWay, NotifyOnValidationError=True, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"
					IsEnabled="{Binding bIsEnabled}"
					ToolTip="Informar o representante do orçamento de vendas" TableView="vwCondicaoPagamento" FieldPesquisa="ID" NameWindowCadastro="WinCondicaoPagamento">
                    <Components:CustomPesquisa.Items>
                        <System:String>xCondicaoPagamento</System:String>
                        <System:String>xDescricao</System:String>
                    </Components:CustomPesquisa.Items>
                </Components:CustomPesquisa>
            </Grid>
        </Expander>
        <TabControl Grid.Row="1" x:Name="tcPrincipal">
            <TabItem>
                <TabItem.HeaderTemplate>
                    <DataTemplate>
                        <TextBlock>
                            <Underline>P</Underline>rincipal
                        </TextBlock>
                    </DataTemplate>
                </TabItem.HeaderTemplate>
                <AdornerDecorator>
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition></ColumnDefinition>
                            <ColumnDefinition></ColumnDefinition>
                        </Grid.ColumnDefinitions>
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                            </Grid.RowDefinitions>
                            <Expander Header="Contato" IsExpanded="True" Style="{StaticResource ExpanderPadrao}">
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="10"></ColumnDefinition>
                                        <ColumnDefinition MaxWidth="150" MinWidth="150"></ColumnDefinition>
                                        <ColumnDefinition MaxWidth="325" MinWidth="325"></ColumnDefinition>
                                        <ColumnDefinition></ColumnDefinition>
                                        <ColumnDefinition></ColumnDefinition>
                                        <ColumnDefinition Width="10"></ColumnDefinition>
                                    </Grid.ColumnDefinitions>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="10"></RowDefinition>
                                        <RowDefinition Height="Auto" MaxHeight="23"></RowDefinition>
                                        <RowDefinition Height="Auto" MaxHeight="23"></RowDefinition>
                                    </Grid.RowDefinitions>

                                    <!--Label-->

                                    <Components:CustomTextBlock Text="Contato" Grid.Column="1" Grid.Row="1"></Components:CustomTextBlock>
                                    <Components:CustomTextBlock Text="Ramo da Atividade" Grid.Column="1" Grid.Row="6"></Components:CustomTextBlock>

                                    <!--Components-->

                                    <Components:CustomPesquisa 
                                        Height="20" Margin="0,0,0,3" Grid.Column="2" Grid.Row="1"                                                           
                                        NameWindowCadastro="WinContato"
			                            xIdPesquisa="{Binding currentModel.idContato, Mode=TwoWay, NotifyOnValidationError=True, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"
			                            IsEnabled="{Binding bIsEnabled}"
			                            ToolTip="Informar a pessoa de contato externo para quem o orçamento é emitido" TableView="vwContato" FieldPesquisa="ID">
                                        <Components:CustomPesquisa.Items>
                                            <System:String>xNome</System:String>
                                            <System:String>xDepartamento</System:String>
                                            <System:String>xTelefoneComercial</System:String>
                                        </Components:CustomPesquisa.Items>
                                    </Components:CustomPesquisa>
                                    <Components:CustomPesquisa 
                                        Height="20" Margin="0,0,0,3" Grid.Column="2" Grid.Row="2"                                                           
                                        NameWindowCadastro="WinContato"
			                            xIdPesquisa="{Binding currentModel.idRamoAtividade, Mode=TwoWay, NotifyOnValidationError=True, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"
			                            IsEnabled="{Binding bIsEnabled}"
			                            TableView="vwRamoAtividade" FieldPesquisa="ID">
                                        <Components:CustomPesquisa.Items>
                                            <System:String>xRamo</System:String>
                                            <System:String>xDescricao</System:String>
                                        </Components:CustomPesquisa.Items>
                                    </Components:CustomPesquisa>

                                </Grid>
                            </Expander>
                            <Expander Header="Status" IsExpanded="True" Grid.Row="1" Style="{StaticResource ExpanderPadrao}">
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="10"></ColumnDefinition>
                                        <ColumnDefinition MaxWidth="180" MinWidth="180"></ColumnDefinition>
                                        <ColumnDefinition MaxWidth="295" MinWidth="295"></ColumnDefinition>
                                        <ColumnDefinition></ColumnDefinition>
                                        <ColumnDefinition></ColumnDefinition>
                                        <ColumnDefinition Width="10"></ColumnDefinition>
                                    </Grid.ColumnDefinitions>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="10"></RowDefinition>
                                        <RowDefinition Height="Auto" MaxHeight="23"></RowDefinition>
                                        <RowDefinition Height="Auto" MaxHeight="23"></RowDefinition>
                                        <RowDefinition Height="Auto" MaxHeight="23"></RowDefinition>
                                        <RowDefinition Height="Auto" MaxHeight="23"></RowDefinition>
                                        <RowDefinition Height="Auto" MaxHeight="23"></RowDefinition>
                                    </Grid.RowDefinitions>

                                    <!--Label-->

                                    <Components:CustomTextBlock Text="Tipo do Orçamento" Grid.Column="1" Grid.Row="1"></Components:CustomTextBlock>
                                    <Components:CustomTextBlock Text="Data de Vencimento" Grid.Column="1" Grid.Row="2"></Components:CustomTextBlock>
                                    <Components:CustomTextBlock Text="Data de Acompanhamento" Grid.Column="1" Grid.Row="3"></Components:CustomTextBlock>
                                    <Components:CustomTextBlock Text="Data de Confirmação" Grid.Column="1" Grid.Row="4"></Components:CustomTextBlock>
                                    <Components:CustomTextBlock Text="Motivo da Perda" Grid.Column="1" Grid.Row="5"></Components:CustomTextBlock>

                                    <!--Components-->

                                    <Components:CustomPesquisa 
                                        Height="20" Margin="0,0,0,3" Grid.Column="2" Grid.Row="1"                                                        
			                            xIdPesquisa="{Binding currentModel.idTipoOrcamento, Mode=TwoWay, NotifyOnValidationError=True, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"
			                            IsEnabled="{Binding bIsEnabled}"
			                            TableView="vwTipoOrcamento" FieldPesquisa="ID">
                                        <Components:CustomPesquisa.Items>
                                            <System:String>xTipoOrcamento</System:String>
                                            <System:String>xDescricao</System:String>
                                        </Components:CustomPesquisa.Items>
                                    </Components:CustomPesquisa>
                                    <Components:HlpDatePicker
                                        Grid.Column="2" Grid.Row="2" Height="20" Margin="0,0,0,3"
                                        xTextDate="{Binding currentModel.dVencimento, Mode=TwoWay, NotifyOnValidationError=True, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"
				                        IsEnabled="{Binding bIsEnabled}"
				                        ToolTip="A data em que o orçamento de venda expira e não é mais válida">
                                    </Components:HlpDatePicker>
                                    <Components:HlpDatePicker
                                        Grid.Column="2" Grid.Row="3" Height="20" Margin="0,0,0,3"
                                        xTextDate="{Binding currentModel.dAcompanhamento, Mode=TwoWay, NotifyOnValidationError=True, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"
				                        IsEnabled="{Binding bIsEnabled}"
				                        ToolTip="A data inicial de qualquer atividade de acompanhamento da cotação de venda">
                                    </Components:HlpDatePicker>
                                    <Components:HlpDatePicker
                                        Grid.Column="2" Grid.Row="4" Height="20" Margin="0,0,0,3"
                                        xTextDate="{Binding currentModel.dConfirmacao, Mode=TwoWay, NotifyOnValidationError=True, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"
				                        IsEnabled="{Binding bIsEnabled}"
				                        ToolTip="A data em que o orçamento de venda expira e não é mais válida">
                                    </Components:HlpDatePicker>
                                    <Components:CustomPesquisa 
                                        Height="20" Margin="0,0,0,3" Grid.Column="2" Grid.Row="5"                                                        
			                            xIdPesquisa="{Binding currentModel.idTipoOrcamento, Mode=TwoWay, NotifyOnValidationError=True, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"			                            
			                            TableView="vwMotivoPerdaOrcam" FieldPesquisa="ID">
                                        <Components:CustomPesquisa.Items>
                                            <System:String>xMotivo</System:String>
                                            <System:String>xDescricao</System:String>
                                        </Components:CustomPesquisa.Items>
                                        <Components:CustomPesquisa.Style>
                                            <Style BasedOn="{StaticResource TextBox_PESQUISA}" TargetType="TextBox">
                                                <Setter Property="FrameworkElement.IsEnabled" Value="False"/>
                                                <Style.Triggers>
                                                    <MultiDataTrigger>
                                                        <MultiDataTrigger.Conditions>
                                                            <Condition Binding="{Binding bIsEnabled}" Value="True"></Condition>
                                                            <Condition Binding="{Binding currentModel.stOrcamento}" Value="3"></Condition>
                                                        </MultiDataTrigger.Conditions>
                                                        <Setter Property="FrameworkElement.IsEnabled" Value="True"/>
                                                    </MultiDataTrigger>
                                                    <MultiDataTrigger>
                                                        <MultiDataTrigger.Conditions>
                                                            <Condition Binding="{Binding bIsEnabled}" Value="True"></Condition>
                                                            <Condition Binding="{Binding currentModel.stOrcamento}" Value="4"></Condition>
                                                        </MultiDataTrigger.Conditions>
                                                        <Setter Property="FrameworkElement.IsEnabled" Value="True"/>
                                                    </MultiDataTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </Components:CustomPesquisa.Style>
                                    </Components:CustomPesquisa>
                                </Grid>
                            </Expander>
                            <Expander Header="Pedido de Compras do Cliente" IsExpanded="True" Grid.Row="2" Style="{StaticResource ExpanderPadrao}">
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="10"></ColumnDefinition>
                                        <ColumnDefinition MaxWidth="290" MinWidth="290"></ColumnDefinition>
                                        <ColumnDefinition MaxWidth="100" MinWidth="100"></ColumnDefinition>
                                        <ColumnDefinition></ColumnDefinition>
                                        <ColumnDefinition Width="10"></ColumnDefinition>
                                    </Grid.ColumnDefinitions>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="10"></RowDefinition>
                                        <RowDefinition Height="Auto" MaxHeight="23"></RowDefinition>
                                        <RowDefinition Height="Auto" MaxHeight="23"></RowDefinition>
                                    </Grid.RowDefinitions>

                                    <!--Label-->

                                    <Components:CustomTextBlock Text="Cotação de compras do cliente" Grid.Column="1" Grid.Row="1"></Components:CustomTextBlock>
                                    <Components:CustomTextBlock Text="Referência do pedido de compras do cliente" Grid.Column="1" Grid.Row="2"></Components:CustomTextBlock>

                                    <!--Components-->

                                    <Components:CustomTextBox
                                            Grid.Column="2" Grid.Row="1"
                                            Text="{Binding currentModel.xCotacaoComprasCliente, Mode=TwoWay, NotifyOnValidationError=True, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"
											IsEnabled="{Binding bIsEnabled}" ToolTip="Informar a cotação de compras do cliente quando a situação for Vendido">
                                    </Components:CustomTextBox>
                                    <Components:CustomTextBox
