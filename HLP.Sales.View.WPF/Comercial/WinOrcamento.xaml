                                </AdornerDecorator>
                            </TabItem>
                        </TabControl>
                    </Grid>
                </AdornerDecorator>
            </TabItem>
            <TabItem GotFocus="TabItem_GotFocus">
                <TabItem.HeaderTemplate>
                    <DataTemplate>
                        <TextBlock>
                            Co<Underline>m</Underline>issões
                        </TextBlock>
                    </DataTemplate>
                </TabItem.HeaderTemplate>
                <ListView ItemsSource="{Binding Source={StaticResource cvsComissoes}}" x:Name="vwComissoes">
                    <ListView.View>
                        <GridView>
                            <GridViewColumn Header="Produto" DisplayMemberBinding="{Binding xProduto}" Width="300"></GridViewColumn>
                            <GridViewColumn Header="% Comissão" DisplayMemberBinding="{Binding pComissao, Converter={StaticResource porcentagemConverter}}" Width="100"></GridViewColumn>
                            <GridViewColumn Header="R$ Comissão" DisplayMemberBinding="{Binding vComissao, Converter={StaticResource moedaConverter}}" Width="100"></GridViewColumn>
                        </GridView>
                    </ListView.View>

                    <ListView.GroupStyle>
                        <GroupStyle>
                            <GroupStyle.ContainerStyle>
                                <Style TargetType="{x:Type GroupItem}">
                                    <Setter Property="Margin" Value="0,0,0,5"/>
                                    <Setter Property="Template">
                                        <Setter.Value>
                                            <ControlTemplate TargetType="{x:Type GroupItem}">
                                                <Expander IsExpanded="True" BorderBrush="#FFA4B97F" 
                                            BorderThickness="0,0,0,1">
                                                    <Expander.Header>
                                                        <Grid>
                                                            <Grid.Resources>
                                                                <convertLocal:SumPorcItemsConverter x:Key="sumPorcItemsConverter"></convertLocal:SumPorcItemsConverter>
                                                            </Grid.Resources>
                                                            <Grid.ColumnDefinitions>
                                                                <ColumnDefinition Width="300"></ColumnDefinition>
                                                                <ColumnDefinition Width="100"></ColumnDefinition>
                                                                <ColumnDefinition Width="100"></ColumnDefinition>
                                                            </Grid.ColumnDefinitions>
                                                            <TextBlock FontWeight="Bold" Text="{Binding Path=Name}" Grid.Column="0"/>
                                                            <TextBlock FontWeight="Bold" Text="{Binding Path=Items, 
                                                                Converter={StaticResource sumPorcItemsConverter}, ConverterParameter=porc}" Grid.Column="1"></TextBlock>
                                                            <TextBlock FontWeight="Bold" Text="{Binding Path=Items, 
                                                                Converter={StaticResource sumPorcItemsConverter}, ConverterParameter=vlr}" Grid.Column="2"></TextBlock>
                                                        </Grid>
                                                    </Expander.Header>
                                                    <Expander.Content>
                                                        <ItemsPresenter />
                                                    </Expander.Content>
                                                </Expander>
                                            </ControlTemplate>
                                        </Setter.Value>
                                    </Setter>
                                </Style>
                            </GroupStyle.ContainerStyle>
                        </GroupStyle>
                    </ListView.GroupStyle>
                </ListView>
            </TabItem>
        </TabControl>
    </Grid>
</runtime:WindowsBase>
